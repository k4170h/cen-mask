{
  "manifest_version": 3,

  "name": "uncen",
  "description": "uncensoring my art",
  "version": "1.0",

  "options_ui": {
    "page": "options.html"
  },

  "action": {
    "default_icon": "icon.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/vendor.js", "js/content_script.js"],
      "run_at":"document_end"
    }
  ],

  "background": {
    "service_worker": "js/background.js"
  },

  "permissions": ["activeTab", "contextMenus", "tabs"],
  "host_permissions": ["<all_urls>"]
}
